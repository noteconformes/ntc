{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", "apps/es/.env*"],
      "outputs": [".nuxt/**", ".output/**", "dist/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "tsc": {},
    "clean": {
      "cache": false
    },
    "biome:check": {
      "cache": false
    }
  }
}
